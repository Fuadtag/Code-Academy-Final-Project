@model Gallery

<!-- Page Title-->
<div class="rn-page-title">
    <div class="rn-pt-overlayer"></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="rn-page-title-inner">
                    <h1>Gallery</h1>
                    <p>Cras eros lorem, rhoncus ac risus sit amet, fringilla ultrices purus.</p>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End Page Title-->
<!-- Page Content-->
<div class="rn-section rn-lightbox-images">
    <div class="container">
        <div class="row">
            @foreach (var item in Model.GalleryItems)
            {
                <div class="col-6 col-sm-4">
                    <a class="rn-lightbox-image" href="~/images/@item.Photo">
                        <img class="img-fluid" src="~/images/@item.PhotoSm" >
                    </a>
                </div>
            }

           
        </div>
    </div>
</div>
<!-- End Page Content-->


