@model  Admin
@{
    ViewData["Title"] = "Index";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Login V6</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminDashboard/Login/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminDashboard/Login/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminDashboard/Login/fonts/iconic/css/material-design-iconic-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminDashboard/Login/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminDashboard/Login/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminDashboard/Login/vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminDashboard/Login/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminDashboard/Login/vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminDashboard/Login/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/AdminDashboard/Login/css/main.css">
    <!--===============================================================================================-->
</head>
<body>

    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100 p-t-85 p-b-20">
                <form class="login100-form validate-form" asp-action="Index" asp-controller="Login" method="post">
                    <div asp-validation-summary="ModelOnly"></div>
                    <span class="login100-form-title p-b-70">
                        Welcome
                    </span>
                    

                    <div class="wrap-input100 validate-input m-t-85 m-b-35" data-validate="Enter username">
                        
                        <input class="input100" type="email" asp-validation-for="Email" asp-for="Email" >
                        <span class="focus-input100" data-placeholder="email" ></span>
                    </div>

                    <div class="wrap-input100 validate-input m-b-50" data-validate="Enter password" >
                        <input class="input100" type="password"  asp-validation-for ="Password" asp-for="Password" >
                        <span class="focus-input100" data-placeholder="Password"></span>
                    </div>

                    <div class="container-login100-form-btn">
                        <button class="login100-form-btn" type ="submit" >
                            Login
                        </button>
                    </div>

                    
                </form>
            </div>
        </div>
    </div>



    <!--===============================================================================================-->
    <script src="~/AdminDashboard/Login/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/AdminDashboard/Login/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/AdminDashboard/Login/vendor/bootstrap/js/popper.js"></script>
    <script src="~/AdminDashboard/Login/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/AdminDashboard/Login/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/AdminDashboard/Login/vendor/daterangepicker/moment.min.js"></script>
    <script src="~/AdminDashboard/Login/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="~/AdminDashboard/Login/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="~/AdminDashboard/Login/js/main.js"></script>

</body>
</html>

